		<OutputType>Exe</OutputType>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>Demo</RootNamespace>
        <NacosSdkCsharpPackageVersion>1.3.1</NacosSdkCsharpPackageVersion>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="nacos-sdk-csharp" Version="$(NacosSdkCsharpPackageVersion)" />
        <PackageReference Include="Refit.HttpClientFactory" Version="8.0.0" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="8.0.0" />
		<PackageReference Include="Volo.Abp.Core" Version="8.1.3" />
        <PackageReference Include="Volo.Abp.Autofac" Version="8.1.3" />
    </ItemGroup>

    <ItemGroup>
        <Compile Include="../../utils/Nacos.Extensions.DiscoveryHandler/NacosDiscoveryHttpClientHandler.cs" />
    </ItemGroup>
</Project>
